{
  "name": "ix-libs",
  "version": "17.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "fresh": "rimraf node_modules && rimraf package-lock.json && npm i",
    "build": "rimraf dist; ng build ix-libs --configuration production",
    "pack": "cd dist/ix-libs && npm pack",
    "package": "npm run build && npm run pack",
    "readme": "cp .README.md /projects/ix-libs",
    "publish": "cd dist/ix-libs && npm publish",
    "library-deps": "cd projects/ix-libs && npm install --legacy-peer-deps",
    "license-checker": "license-checker  --relativeLicensePath --production --json --out license.json",
    "update-cli": "ng update @angular/cli --force -C",
    "update-core": "ng update @angular/core --force -C",
    "update-material": "ng update @angular/material --force -C",
    "update-rxjs": "ng update rxjs --force -C",
    "update-all": "npm run update-cli && npm run update-core && npm run update-material && npm run update-rxjs"
  },
  "dependencies": {
    "@angular-material-components/color-picker": "^7.0.1",
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^17.3.6",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^17.3.6",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "material": "^0.4.3",
    "ngx-json-viewer": "^3.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^0.0.28",
    "concurrently": "^7.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^18.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~5.4.5"
  }
}