{
  "name": "ix-libs",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "fresh": "rimraf node_modules && rimraf package-lock.json && npm i",
    "build": "rimraf dist; ng build ix-libs --configuration production",
    "pack": "cd dist/ix-libs && npm pack",
    "package": "npm run build && npm run pack",
    "readme": "cp .README.md /projects/ix-libs",
    "publish": "cd dist/ix-libs && npm publish",
    "license-checker": "license-checker  --relativeLicensePath --production --json --out license.json",
    "update-cli": "ng update @angular/cli --force -C",
    "update-core": "ng update @angular/core --force -C",
    "update-material": "ng update @angular/material --force -C",
    "update-rxjs": "ng update rxjs --force -C",
    "update-all": "npm run update-cli && npm run update-core && npm run update-material && npm run update-rxjs"
  },
  "dependencies": {
    "@angular-material-components/color-picker": "^7.0.1",
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "material": "^0.4.3",
    "ngx-json-viewer": "^3.0.2",
    "rxjs": "~7.5.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0-next.1",
    "concurrently": "^1.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.0.8",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.4.4"
  }
}