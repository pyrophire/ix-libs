{
  "name": "ix-libs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "rimraf dist; ng build ix-libs --prod",
    "pack": "cd dist/ix-libs && npm pack",
    "package": "npm run build && npm run pack",
    "readme": "cp .README.md /projects/ix-libs",
    "publish": "cd dist/ix-libs && npm publish",
    "license-checker": "license-checker  --relativeLicensePath --production --json --out license.json",
    "update-cli": "ng update @angular/cli --force -C",
    "update-core": "ng update @angular/core --force -C",
    "update-material": "ng update @angular/material --force -C",
    "update-rxjs": "ng update rxjs --force -C",
    "update-all": "npm run update-cli && npm run update-core && npm run update-material && npm run update-rxjs"
  },
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.0",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "material": "^0.4.3",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0-next.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}