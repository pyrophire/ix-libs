{
  "name": "@pyrophire/ix-libs",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "fresh": "rimraf node_modules && rimraf package-lock.json && npm i",
    "build": "rimraf dist; ng build ix-libs --configuration production",
    "pack": "cd dist/ix-libs && npm pack",
    "package": "npm run build && npm run pack",
    "readme": "cp .README.md /projects/ix-libs",
    "publish": "cd dist/ix-libs && npm publish",
    "pack:publish": "npm run package && npm run publish",
    "library-deps": "cd projects/ix-libs && npm install --legacy-peer-deps",
    "license-checker": "license-checker  --relativeLicensePath --production --json --out license.json",
    "update-cli": "ng update @angular/cli --force -C",
    "update-core": "ng update @angular/core --force -C",
    "update-material": "ng update @angular/material --force -C",
    "update-rxjs": "ng update rxjs --force -C",
    "update-all": "npm run update-cli && npm run update-core && npm run update-material && npm run update-rxjs"
  },
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/cdk": "^20.0.1",
    "@angular/cli": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^20.0.1",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^12.20.55",
    "codelyzer": "^0.0.28",
    "concurrently": "^7.6.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^21.0.0",
    "ngx-json-viewer": "^3.2.1",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "typescript": "~5.9.3"
  }
}